package core;
class Person{
	int id;
	String name;
	long contact;
	Person(int id,String name,long contact){
		this.id= id;
		this.name= name;
		this.contact = contact;
	}
	public int getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public long getContact() {
		return contact;
	}
	public void showDatat() {
		System.out.println("id : "+id+" name : "+name+" contact : "+contact);
	}
}
public class P019_Encapsulation {
	public static void main(String[] args) {
		Person p1 = new Person(1, "rohan", 6485321);
//		System.out.println(p1.getId());
//		System.out.println(p1.getName());
//		System.out.println(p1.getContact());
//		p1.id = 13;
		System.out.println(p1);
	}
}
